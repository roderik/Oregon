{
    "name": "umpirsky/oregon",
    "type": "library",
    "description": "Statistics library for open source organizations.",
    "keywords": ["packagist", "api", "composer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com",
            "homepage": "http://umpirsky.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "knplabs/packagist-api": "~1.0",
        "knplabs/github-api": "~1.4",
        "athari/yalinqo": "*"
    },
    "require-dev": {
        "phpspec/phpspec2": "dev-develop"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Oregon\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
